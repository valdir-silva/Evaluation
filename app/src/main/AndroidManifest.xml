<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tribo_mkt.evaluation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.inicio.InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.albuns.AlbunsActivity"
            android:parentActivityName=".presentation.inicio.InicioActivity" />
        <activity
            android:name=".presentation.postagens.PostagensActivity"
            android:parentActivityName=".presentation.inicio.InicioActivity" />
        <activity android:name=".presentation.comentarios.ComentariosActivity" />
        <activity android:name=".presentation.fotos.FotosActivity" />
        <activity android:name=".presentation.fotoDetalhe.FotoDetalheActivity" />
    </application>

</manifest>